<!DOCTYPE html>
<html>
  <head>
    <title>Lancer</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700|Nunito:400,400i,700" rel="stylesheet">
  </head>

  <body>
    <div class="main-content">
      <div class="container">
        <%= render "shared/search" %>
        <%= render "shared/main" %>
        <%= yield %>
        <%= render "shared/pagination"%>
      </div>
    </div>
    
  </body>
</html>
